@model IEnumerable<HBR.Persistence.Models.Categories>


<div class="container-fluid">
    <div class="row clearfix">
        <div class="col-md-12">
            <div class="card patients-list">
                <div class="body">
                    <div class="tab-content m-t-10">
                        <div class="tab-pane table-responsive active" id="All">
                            <table id="example"class="table table-striped table-bordered" style="width:100%; align-content:center">
                                <thead>
                                    <tr class="col-sm-10">
                                        <th>Categorias</th>
                                    </tr>
                                </thead>
                                <tbody class="col-md-2">

                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>
                                                @Html.DisplayFor(modelItem => item._Name)
                                            </td>
                                        </tr>

                                    }
                                </tbody>
                            </table>
                        </div>


                    </div>
                </div>
            </div>
        </div>
    </div>

</div>


<script src="~/js/jquery-3.3.1.min.js"></script>
<script src="~/js/jquery-1.10.2.min.js"></script>

<script type="text/javascript">
    $(document).ready(function () {
        $('#example').DataTable();



    });


</script>



